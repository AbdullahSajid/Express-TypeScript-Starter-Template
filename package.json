{
  "name": "express-typescript-starter-template",
  "version": "1.0.0",
  "description": "Express.js Starter Template based on Typescript",
  "main": "src/index.ts",
  "scripts": {
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\"",
    "lint-and-fix": "tsc --noEmit && eslint . --ext .js,.ts --quiet --fix",
    "test": "set DEBUG=* & nodemon src/index.ts",
    "dev": "nodemon src/index.ts",
    "build": "rimraf dist && tsc",
    "serve": "node dist/index.js",
    "build-and-serve": "npm run build && npm run serve"
  },
  "author": "Abdullah Sajid",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.4",
    "mongoose-paginate-v2": "^1.6.3",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.1",
    "rate-limiter-flexible": "^2.3.6",
    "request-ip": "^2.1.3",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose-paginate-v2": "^1.4.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@types/request-ip": "0.0.37",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.6.2"
  }
}
